{
  "name": "nestjs-study",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "NODE_ENV=local nest start --watch",
    "docker:up": "docker compose -f docker-compose.local.yml up --build -d",
    "docker:down": "docker compose -f docker-compose.local.yml down",
    "typeorm-ext": "cross-env NODE_ENV=${npm_config_env:-local} ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs -d typeorm.config.ts",
    "entity:drop": "NODE_ENV=${npm_config_env:-test} ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:drop -d ./typeorm.config.ts",
    "entity:sync": "NODE_ENV=${npm_config_env:-test} ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js schema:sync -d ./typeorm.config.ts",
    "seed": "cross-env npm run typeorm-ext seed:run",
    "db:reset": "npm run entity:drop && npm run entity:sync && npm run seed",
    "mock:server": "prism mock http://localhost:3000/api-json -p 4010",
    "test": "jest --config \"$(pwd)/test/jest-unit.js\"",
    "test:watch": "jest --config \"$(pwd)/test/jest-unit.js\" --watch",
    "test:cov": "jest --config \"$(pwd)/test/jest-unit.js\" --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config \"$(pwd)/test/jest-unit.js\"",
    "test:e2e": "npm run test:db:up && jest --config \"$(pwd)/test/jest-e2e.json\" && npm run test:db:down",
    "test:integration": "npm run test:db:up && jest --config \"$(pwd)/test/jest-integration.js\" && npm run test:db:down",
    "test:all": "npm run test && npm run test:integration && npm run test:e2e",
    "test:db:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker-compose -f docker-compose.test.yml down -v"
  },
  "dependencies": {
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cache-manager": "^4.0.6",
    "@types/cache-manager-redis-store": "^2.0.4",
    "@types/cookie-parser": "^1.4.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/redis": "^4.0.10",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "cache-manager-ioredis-yet": "^2.1.2",
    "cache-manager-redis-yet": "^5.1.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "cross-env": "^10.0.0",
    "mongoose": "^8.18.1",
    "mysql2": "^3.14.5",
    "nest-winston": "^1.10.2",
    "nestjs-cls": "^6.0.1",
    "passport-jwt": "^4.0.1",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.26",
    "typeorm-extension": "^3.7.1",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional": "^0.5.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@stoplight/prism-cli": "^4.10.5",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
